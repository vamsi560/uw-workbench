# Frontend Environment Variables for Java Backend Integration

# Local Development - Java Backend
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_POLL_URL=http://localhost:8080/api/workitems/poll
NEXT_PUBLIC_HEALTH_URL=http://localhost:8080/api/health
NEXT_PUBLIC_LLM_STATUS_URL=http://localhost:8080/api/llm/status

# Production - Java Backend (update with your actual Java backend URL)
# NEXT_PUBLIC_API_URL=https://your-java-backend-domain.com
# NEXT_PUBLIC_POLL_URL=https://your-java-backend-domain.com/api/workitems/poll
# NEXT_PUBLIC_HEALTH_URL=https://your-java-backend-domain.com/api/health
# NEXT_PUBLIC_LLM_STATUS_URL=https://your-java-backend-domain.com/api/llm/status

# Fallback URLs (original Python backend for rollback)
# NEXT_PUBLIC_API_URL=https://uw-workbench-jade.vercel.app
# NEXT_PUBLIC_POLL_URL=https://uw-workbench-jade.vercel.app/api/workitems/poll

# Optional: Feature flags for gradual migration
NEXT_PUBLIC_USE_JAVA_BACKEND=true
NEXT_PUBLIC_ENABLE_ENHANCED_FEATURES=true